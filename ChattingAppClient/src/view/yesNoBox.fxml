<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="63.0" prefWidth="347.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.YesNoBoxController">
    <children>
      <BorderPane prefHeight="200.0" prefWidth="347.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <bottom>
                <HBox prefHeight="39.0" prefWidth="438.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="YesButton" mnemonicParsing="false" onAction="#onYes" prefHeight="28.0" prefWidth="143.0" text="yes">
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </HBox.margin></Button>
                        <Button fx:id="NoButton" mnemonicParsing="false" onAction="#onNo" prefHeight="28.0" prefWidth="141.0" text="No">
                     <HBox.margin>
                        <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
                     </HBox.margin></Button>
                    </children>
                </HBox>
         </bottom>
         <center>
            <Label fx:id="messageLabel" text="Label" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
    </children>
</AnchorPane>

<AnchorPane id="AnchorPane" prefHeight="567.0" prefWidth="302.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainPageFormController">
   <children>
      <MenuBar prefHeight="25.0" prefWidth="305.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <ImageView id="imageView" fx:id="imageView" fitHeight="72.0" fitWidth="130.0" layoutX="35.0" layoutY="38.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@myImg.jpg" />
         </image></ImageView>
      <ComboBox id="statusComboBox" fx:id="statusComboBox" layoutX="145.0" layoutY="85.0" prefHeight="25.0" prefWidth="107.0" onAction="#statusChangeHandeling" />

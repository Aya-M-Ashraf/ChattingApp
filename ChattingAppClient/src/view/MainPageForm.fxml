      <ListView fx:id="listView" layoutY="184.0" prefHeight="381.0" prefWidth="309.0" />
      <Label fx:id="testLabel" layoutX="40.0" layoutY="146.0" prefHeight="25.0" prefWidth="63.0" />
      <Button id="addFriendButton" fx:id="addFriendButton" layoutX="206.0" layoutY="159.0" mnemonicParsing="false" onMouseClicked="#handleAddFriendButton" prefHeight="25.0" prefWidth="99.0" text="+ Add Friends">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>

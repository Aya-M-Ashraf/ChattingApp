<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="312.0" prefWidth="469.0" stylesheets="@serverform.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ServerFormController">
   <children>
      <TabPane prefHeight="318.0" prefWidth="469.0" stylesheets="@serverform.css" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Start/Stop">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="244.0" prefWidth="381.0">
                     <children>
                        <PieChart fx:id="pieChart" layoutY="-1.0" prefHeight="291.0" prefWidth="256.0" />
                        <Button id="button" fx:id="stopServiceButton" layoutX="268.0" layoutY="17.0" mnemonicParsing="false" onMouseClicked="#handleStopButton" prefHeight="28.0" prefWidth="84.0" text="Stop" />
                        <Button id="button" fx:id="startServiceButton" layoutX="141.0" layoutY="17.0" mnemonicParsing="false" onMouseClicked="#handleStartButton" prefHeight="29.0" prefWidth="80.0" stylesheets="@serverform.css" text="Start" />
                        <Button id="button" fx:id="refreshButton" layoutX="57.0" layoutY="210.0" mnemonicParsing="false" onAction="#updatePieChart" prefHeight="22.0" prefWidth="88.0" stylesheets="@serverform.css" text="Refresh" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Reporting">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
            <Tab id="adTab" fx:id="adTab" text="Sending Ad">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TextArea id="adTextArea" fx:id="adTextArea" layoutX="43.0" layoutY="14.0" prefHeight="200.0" prefWidth="367.0" />
                        <Button id="sendAdButton" fx:id="sendAdButton" layoutX="201.0" layoutY="233.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="89.0" text="Send" onMouseClicked="#handleSendAd" />
                     </children>
                  </AnchorPane>
              </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
